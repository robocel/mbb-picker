  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "@reach/router": "^1.2.1",
    "firebase": "^5.5.6",
    "react": "^16.7.0-alpha.0",
    "react-beautiful-dnd": "^10.0.2",
    "react-dom": "^16.7.0-alpha.0",
